{
  "name": "kecasn",
  "version": "0.3.0",
  "description": "Convert strings between snake_case, camelCase, and kebab-case. Convert object keys and array values with ease.",
  "engines": {
    "node": "^12.20.0 || ^14.13.1 || >=16.0.0"
  },
  "keywords": [
    "string",
    "case",
    "convert",
    "api",
    "camel",
    "snake",
    "kebab"
  ],
  "author": "Ivan Kleshnin <ivan@paqmind.com> (https://paqmind.com)",
  "license": "ISC",
  "scripts": {
    "ts-node": "node --loader ts-node/esm",
    "build": "yarn build:cjs",
    "build:esm": "tsc",
    "build:cjs": "tsc -p tsconfig.cjs.json",
    "test": "jest",
    "lint": "eslint .",
    "prepublishOnly": "yarn build"
  },
  "main": "./dist/cjs/index.js",
  "_module": "./dist/esm/index.js",
  "_exports": "./dist/esm/index.js",
  "types": "dist/cjs/index.d.ts",
  "files": [
    "/dist"
  ],
  "devDependencies": {
    "@types/jest": "^27.0.1",
    "@types/node": "^16.6.1",
    "@typescript-eslint/eslint-plugin": "^4.29.1",
    "@typescript-eslint/parser": "^4.29.1",
    "eslint": "^7.32.0",
    "jest": "^27.0.6",
    "ts-jest": "^27.0.4",
    "ts-node": "^10.2.0",
    "typescript": "^4.3.5"
  }
}
